#!/bin/bash

export PATH=@CMAKE_INSTALL_PREFIX@/@PROJECT_INSTALL_BINDIR@:$PATH
if [[ "$OSTYPE" == "darwin"* ]]; then
    export DYLD_LIBRARY_PATH=@CMAKE_INSTALL_PREFIX@/@PROJECT_INSTALL_LIBDIR@:$DYLD_LIBRARY_PATH
else
    export LD_LIBRARY_PATH=@CMAKE_INSTALL_PREFIX@/@PROJECT_INSTALL_LIBDIR@:$LD_LIBRARY_PATH
fi
