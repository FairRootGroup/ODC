.init --id @TEST_SESSION@
.submit --id @TEST_SESSION@ --plugin odc-rp-same -r "<rms>localhost</rms><agents>1</agents><slots>36</slots>"
.activate --id @TEST_SESSION@ --script "cat @ODC_DATADIR@/ex-topo-infinite.xml"
.prop --id @TEST_SESSION@ --prop key1:value1 key2:value2
.config --id @TEST_SESSION@
.start --id @TEST_SESSION@
.sleep --ms 1000
.stop --id @TEST_SESSION@
.reset --id @TEST_SESSION@
.term --id @TEST_SESSION@
.down --id @TEST_SESSION@
